enabled=PoolStringArray( "res://addons/godot-openvr/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-openvr/godot_openvr.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

